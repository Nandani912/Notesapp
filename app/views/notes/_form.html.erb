<%= form_with model:@note do |form|%>
   <div class="field">
  <%= form.label :title %>
  <%= form.text_field :title %>
  </div>
  <div class="field">
  <%=form.label :description %>
  <%= form.text_area :description %>
  </div>
  <div class="field">
  <%= form.label :date %>
  <%= form.date_field :date %>
  <div class="field">
  <%= form.submit %>
  </div>
  <%end%>
